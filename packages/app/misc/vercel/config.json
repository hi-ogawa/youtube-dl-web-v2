{
  "version": 3,
  "routes": [
    {
      "src": "^/assets/(.*)$",
      "headers": {
        "cross-origin-opener-policy": "same-origin",
        "cross-origin-embedder-policy": "require-corp",
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/(.*)$",
      "dest": "/",
      "check": true
    }
  ]
}
