{
  "type": "module",
  "scripts": {
    "clean": "rm -rf src/worker/build dist .vercel/output",
    "dev": "pnpm build:worker && run-p rakkas:dev tsc:dev",
    "build": "run-s build:worker build:app",
    "build:worker": "esbuild ./src/worker/ffmpeg.ts ./src/worker/libwebm.ts --bundle --outdir=./src/worker/build",
    "build:app": "run-s rakkas:build fix-vercel-output",
    "rakkas:dev": "rakkas --no-clearScreen",
    "rakkas:build": "rakkas build",
    "test": "vitest",
    "e2e": "playwright test",
    "tsc": "tsc",
    "tsc:dev": "tsc --noEmit --watch --preserveWatchOutput",
    "fix-vercel-output": "cp -f misc/vercel/config.json .vercel/output/config.json && cp -f misc/vercel/.vc-config.json .vercel/output/functions/index.func/.vc-config.json",
    "ts": "node --experimental-loader ../../misc/esbuild-register-loader.mjs -r esbuild-register",
    "release": "vercel deploy --prebuilt .",
    "release:production": "vercel deploy --prebuilt . --prod"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.10.3",
    "@hattip/compose": "^0.0.22",
    "@headlessui/react": "^1.7.4",
    "@hiogawa/ffmpeg": "workspace:*",
    "@hiogawa/flac-picture": "workspace:*",
    "@hiogawa/utils": "1.4.2-pre.9",
    "@opentelemetry/api": "^1.3.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.34.0",
    "@opentelemetry/sdk-node": "^0.34.0",
    "@opentelemetry/sdk-trace-base": "^1.8.0",
    "@opentelemetry/semantic-conventions": "^1.8.0",
    "@tanstack/react-query": "^4.3.4",
    "@tanstack/react-query-devtools": "^4.3.5",
    "comlink": "^4.3.1",
    "lodash": "^4.17.21",
    "rakkasjs": "0.6.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.35.0",
    "react-hot-toast": "^2.4.0",
    "react-remove-scroll": "^2.5.5",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@hiogawa/unocss-preset-antd": "2.2.1-pre.2",
    "@iconify-json/ri": "^1.1.3",
    "@playwright/test": "^1.28.0",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@unocss/reset": "^0.46.1",
    "undici": "^5.12.0",
    "unocss": "0.46.5",
    "vite": "^3.1.1"
  }
}
