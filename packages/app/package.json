{
  "type": "module",
  "scripts": {
    "clean": "rm -rf src/worker/build dist .vercel/output",
    "dev": "pnpm build:worker && run-p dev:*",
    "dev-e2e": "pnpm build:worker && NODE_ENV=test pnpm dev:vite --port 15173",
    "dev:vite": "vite",
    "build": "run-s build:*",
    "build:worker": "esbuild ./src/worker/ffmpeg.ts ./src/worker/libwebm.ts --bundle --outdir=./src/worker/build",
    "build:vite": "vite build && vite build --ssr",
    "build:cloudflare": "bash misc/cloudflare/build.sh",
    "test": "vitest",
    "test-e2e": "playwright test",
    "tsc": "tsc",
    "dev:tsc": "tsc --noEmit --watch --preserveWatchOutput",
    "ts": "node --experimental-loader ../../misc/esbuild-register-loader.mjs -r esbuild-register",
    "dotenv-staging": "bash misc/dotenv.sh .env.staging",
    "dotenv-production": "bash misc/dotenv.sh .env.production",
    "preview": "wrangler dev --env staging",
    "release": "wrangler deploy --env staging",
    "release-production": "wrangler deploy"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.332.0",
    "@aws-sdk/s3-presigned-post": "^3.332.0",
    "@aws-sdk/s3-request-presigner": "^3.332.0",
    "@cloudflare/kv-asset-handler": "^0.3.0",
    "@floating-ui/react-dom-interactions": "^0.10.3",
    "@hattip/compose": "^0.0.22",
    "@headlessui/react": "^1.7.4",
    "@hiogawa/ffmpeg": "workspace:*",
    "@hiogawa/flac-picture": "workspace:*",
    "@hiogawa/query-proxy": "0.1.1-pre.0",
    "@hiogawa/tiny-rpc": "^0.2.2",
    "@hiogawa/utils-experimental": "^0.1.0",
    "@hiogawa/utils-react": "1.2.1-pre.4",
    "@tanstack/react-query": "^4.29.5",
    "@tanstack/react-query-devtools": "^4.29.6",
    "comlink": "^4.3.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.35.0",
    "react-hot-toast": "^2.4.0",
    "react-remove-scroll": "^2.5.5",
    "react-router-dom": "^6.13.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@hattip/adapter-node": "^0.0.34",
    "@hiogawa/unocss-preset-antd": "2.2.1-pre.2",
    "@hiogawa/vite-glob-routes": "0.3.1",
    "@hiogawa/vite-import-index-html": "^0.2.0",
    "@iconify-json/ri": "^1.1.3",
    "@miniflare/kv": "^2.14.0",
    "@miniflare/storage-file": "^2.14.0",
    "@playwright/test": "^1.31.2",
    "@remix-run/web-file": "^3.0.2",
    "@types/lodash": "^4.14.186",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@unocss/reset": "^0.46.1",
    "@vavite/connect": "^1.8.1",
    "@vitejs/plugin-react": "^4.0.1",
    "react-router": "^6.13.0",
    "undici": "^5.22.1",
    "unocss": "0.46.5",
    "vite": "^4.3.9",
    "wrangler": "^3.1.1"
  }
}
